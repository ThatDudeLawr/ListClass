
template<class T>
Node<T>::Node(T val)
{
    m_data = val;
    m_next = NULL;
}

template<class T>
Node<T>::~Node()
{
}

template<class T>
Node<T>* Node<T>::GetNext() const
{
    return m_next;
}

template<class T>
T Node<T>::GetData() const
{
    return m_data;
}

template<class T>
void Node<T>::SetNext(Node<T>* p)
{
    m_next = p;
}

template<class T>
void Node<T>::SetData(T val)
{
    m_data = val;
}
